{
  "name": "quickjs-typescript",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "update-quickjs": "git subtree pull --prefix=quickjs --squash git@github.com:ldarren/QuickJS.git mod",
    "make-debug": "cd src && make DEBUG=1",
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "yarn make-debug && ts-node ./src/examples/hello.ts",
    "clean": "cd src && make clean"
  },
  "devDependencies": {
    "@types/emscripten": "^1.38.0",
    "@types/node": "^13.1.4",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}
